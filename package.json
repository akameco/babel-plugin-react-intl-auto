{
  "name": "babel-plugin-react-intl-auto",
  "version": "1.0.0",
  "main": "lib/index.js",
  "description": "i18n for the component age. Auto management react-intl ID",
  "license": "MIT",
  "repository": "akameco/babel-plugin-react-intl-auto",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "scripts": {
    "add-contributor": "add-contributor add",
    "precommit": "lint-staged",
    "build": "babel src -d lib --ignore __tests__,__fixtures__",
    "prepare": "npm run build",
    "flow": "flow",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "lint": "eslint src",
    "add:coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --ci --runInBand",
    "test:ci": "npm run validate && npm run add:coveralls",
    "validate": "npm run lint && npm run flow && npm run test:coverage"
  },
  "lint-staged": {
    "src/**/*.js": [
      "fmt",
      "validate",
      "git add"
    ],
    "*.{json,md}": [
      "fmt",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "react-components",
    "react-intl",
    "i18n",
    "react-intl-auto",
    "babel-plugin",
    "auto",
    "babel",
    "plugin",
    "generate",
    "defineMessages"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {
    "babel-types": "^6.26.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.10.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-jest": "^22.0.4",
    "babel-log": "^2.0.0",
    "babel-plugin-tester": "^5.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-precure": "^0.13.0",
    "flow-bin": "^0.63.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2"
  }
}
